{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "env": {
    "KV_REST_API_URL": "@kv_rest_api_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token"
  },
  "regions": ["iad1", "sfo1", "fra1"],
  "rewrites": [
    {
      "source": "/room/:roomId/sync",
      "destination": "/api/room/[roomId]/sync"
    },
    {
      "source": "/health",
      "destination": "/api/health"
    }
  ]
}